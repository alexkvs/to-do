<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd">
    <changeSet author="alexkvs" id="V-4.0-02-4">
        <loadUpdateData tableName="customer"
                        primaryKey="id"
                        file="customer.csv"
                        relativeToChangelogFile="true">
            <column index="0" name="id" type="SKIP"/>
            <column index="1" name="first_name" type="STRING"/>
            <column index="2" name="last_name" type="STRING"/>
            <column index="3" name="email" type="STRING"/>
        </loadUpdateData>
    </changeSet>
</databaseChangeLog>
